{
  "name": "express-02",
  "version": "0.0.0",
  "private": true,
  "main": "src/app.js",
  "scripts": {
    "build": "babel src/ -d dist/",
    "prepublish": "npm run build",
    "flow": "flow",
    "flow:check": "flow check ./src/",
    "start": "npm run build && node --harmony --harmony_promise_finally $NODE_DEBUG_OPTION dist/app.js",
    "dev": "babel-node $NODE_DEBUG_OPTION src/app.js",
    "dev1": "nodemon -V --watch src/ --exec bin/babel-node $NODE_DEBUG_OPTION src/app.js",
    "dev2": "watch --wait=1 'bin/babel src/ -d dist/' ./src/ & nodemon --harmony --harmony_promise_finally $NODE_DEBUG_OPTION ./dist/app.js",
    "eumetsat": "babel-node $NODE_DEBUG_OPTION src/bin/eumetsat.js",
    "scraper": "babel-node $NODE_DEBUG_OPTION src/bin/scrapper.js",
    "db": "babel-node $NODE_DEBUG_OPTION src/bin/db.js",
    "godaddy": "babel-node $NODE_DEBUG_OPTION src/bin/godaddy-dns.js",
    "avahi": "babel-node $NODE_DEBUG_OPTION src/bin/avahi.js"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "bson-ext": "^2.0.0",
    "chalk": "^2.3.2",
    "cli": "^1.0.1",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dbus-as-promised": "^0.1.1",
    "debug": "^2.6.9",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.5",
    "mongoose": "^5.0.13",
    "morgan": "^ 1.9.0",
    "node-sass-middleware": "0.9.8",
    "node-schedule": "^1.3.0",
    "nunjucks": "^3.1.2",
    "opn": "^5.3.0",
    "pg-promise": "^7.5.4",
    "phantom": "^4.0.12",
    "puppeteer": "^1.2.0",
    "serve-favicon": "^2.5.0",
    "tmp": "0.0.33",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.64.0",
    "nodemon": "^1.17.3",
    "watch": "^1.0.2"
  }
}
